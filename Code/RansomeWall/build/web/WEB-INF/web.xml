<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>DatauserRegAction</servlet-name>
        <servlet-class>com.rnswall.actions.DatauserRegAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DatauserLoginAction</servlet-name>
        <servlet-class>com.rnswall.actions.DatauserLoginAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustFileUploadAction</servlet-name>
        <servlet-class>com.rnswall.actions.CustFileUploadAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RansomeuserRegisterAction</servlet-name>
        <servlet-class>com.rnswall.actions.RansomeuserRegisterAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RansomeUserLoginCheck</servlet-name>
        <servlet-class>com.rnswall.actions.RansomeUserLoginCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RansomAttacked</servlet-name>
        <servlet-class>com.rnswall.actions.RansomAttacked</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RansomAttakingTwo</servlet-name>
        <servlet-class>com.rnswall.actions.RansomAttakingTwo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Downolad</servlet-name>
        <servlet-class>com.rnswall.actions.Downolad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataUserFileRecovering</servlet-name>
        <servlet-class>com.rnswall.actions.DataUserFileRecovering</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DatauserRegAction</servlet-name>
        <url-pattern>/DatauserRegAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DatauserLoginAction</servlet-name>
        <url-pattern>/DatauserLoginAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustFileUploadAction</servlet-name>
        <url-pattern>/CustFileUploadAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RansomeuserRegisterAction</servlet-name>
        <url-pattern>/RansomeuserRegisterAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RansomeUserLoginCheck</servlet-name>
        <url-pattern>/RansomeUserLoginCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RansomAttacked</servlet-name>
        <url-pattern>/RansomAttacked</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RansomAttakingTwo</servlet-name>
        <url-pattern>/RansomAttakingTwo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Downolad</servlet-name>
        <url-pattern>/Downolad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataUserFileRecovering</servlet-name>
        <url-pattern>/DataUserFileRecovering</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
